<h1>Songs</h1>

<div><%= link_to 'Create Song', new_song_path %></div>


<table>
  <tbody>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th>Genre</th>
      <th>Artist</th>
      <th></th>
    </tr>
    <% @songs.each do |song| %>
    <tr>
      <p><%= song.title %></p>
      <p><%= song.release_year %></p>
      <p><%= song.genre %></p>
      <p><%= song.artist_name%></p>
      <p>
        <%= link_to 'View', song %> |
        <%= link_to 'Edit', edit_song_path(song) %>
        <%= link_to "Delete", song, method: :delete, data: { confirm: "Really?" } %>
      </p>
    </tr>
  </tbody>

</table>



<% end %>
